"xrandr --output DisplayPort-0 --right-of HDMI-A-O  --output DisplayPort-1 --right-of HDMI-A-0 --primary --output HDMI-A-0 --preferred --output eDP --off"
    m:0x40 + c:133

"konsole -e julia"
   XF86Calculator

"caja ~/"
   Mod4+E

"fzf-files-popup"
   Control+Shift+o

"mate-terminal"
   Mod4+T
