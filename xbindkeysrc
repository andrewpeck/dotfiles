# Get a key combination with:
# xbindkeys --multikey

"xcalib"
    Control+Alt+i

"slack --disable-gpu"
    Control+Shift+s

"mpc toggle"
    Control+Alt+p

"mpc next"
    Control+Alt+bracketright

"mpc prev"
    Control+Alt+bracketleft

"spectacle -bcr"
    Print

"ghostty -e ipython3"
   Mod2+Mod4+c

"dolphin ~/"
   Mod2+Mod4+f

"dolphin ~/"
   Mod2+Mod4+e

"emacsclient -nc -a 'emacs'"
   Mod2+Mod4+r

"ghostty"
   Mod2+Mod4+t

"sh ~/dotfiles/scripts/emacs-everywhere"
   F11

"spectacle -bcr"
   Print

"sh ~/dotfiles/scripts/select-emacs"
    F9

"fzf-files-popup"
   Control + o
