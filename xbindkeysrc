# Get a key combination with:
# xbindkeys --multikey

"xcalib -alter -invert"
    Control+Alt+i

"restart_skype"
    Control+Alt+k

"slack --disable-gpu"
    Control+Shift+s
"skypeforlinux --disable-gpu"
    Control+Shift+y

"mpc toggle"
    Control+Alt+p
"mpc next"
    Control+Alt+bracketright
"mpc prev"
    Control+Alt+bracketleft

#"spectacle -bcr"
"mate-screenshot -a -c"
    m:0x0 + c:107

#"mate-screenshot -a -c"
    #m:0x0 + c:107
